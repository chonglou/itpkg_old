<!DOCTYPE html>
<html lang="zh-CN">
#set(Map<String,String> gl_site)
#set(gl_session=session.d1s7e0wp)
<head>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta name="description" content="${gl_site.description}"/>
    <meta name="keywords" content="${gl_site.keywords}"/>
    <meta name="author" content="${gl_site.author}">
    <title>${gl_site.title}#if(title)-${title}#end</title>
    <link href="/bootstrap/css/bootstrap.min.css" rel="stylesheet"/>
    <link href="/bootstrap/css/bootstrap-responsive.min.css" rel="stylesheet"/>
    <script type="text/javascript" src="/js/config.js#if(gl_debug)?_=${random()}#end"></script>
    <script type="text/javascript" src="/jquery/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="/ueditor/ueditor.all.min.js"></script>
    <script type="text/javascript" src="/bootstrap/js/bootstrap.min.js"></script>
    <link href="/style/css/main.css#if(gl_debug)?_=${random()}#end" rel="stylesheet"/>
    #if(gl_captcha == "reCaptcha")
        <script type="text/javascript" src="http://www.google.com/recaptcha/api/js/recaptcha_ajax.js"></script>
        <script type="text/javascript">
            function showRecaptcha(element) {
                Recaptcha.create(
                    "${gl_reCaptcha_key}", element, {
                    theme: "red",
                    callback: Recaptcha.focus_response_field}
                );
            }
        </script>
    #end
    <script type="text/javascript">
        var gl_captcha="${gl_captcha}";
    </script>
    <script type="text/javascript" src="/js/widgets.js#if(gl_debug)?_=${random()}#end"></script>
    <script type="text/javascript" src="/js/main.js#if(gl_debug)?_=${random()}#end"></script>
    #if(gl_debug)
    <script type="text/javascript" src="/js/test.js?_=${random()}"></script>
    #end
    #if(gl_session)
    <script type="text/javascript" src="/js/personal.js#if(gl_debug)?_=${random()}#end"></script>
    #else
    <script type="text/javascript" src="/js/non-login.js#if(gl_debug)?_=${random()}#end"></script>
    #end
    #macro($!head)#end
</head>
<body>
<div class="navbar navbar-fixed-top  navbar-inverse">
    <div class="navbar-inner">
        <div class="container-fluid">
            <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                <span class="icon-bar">&nbsp;</span>
                <span class="icon-bar">&nbsp;</span>
                <span class="icon-bar">&nbsp;</span>
            </a>
            <a class="brand" href="/">${gl_site.title}</a>
            #macro($!personal_bar)#end
            #macro($!top_nav_bar)#end
        </div>
    </div>
</div>
<div class="container-fluid">
    <div class="row-fluid">
        <div class="span3">
            <!--#macro($!left)-->
            <!--#end-->
        </div>
        <div class="span9">
            <div id="gl_message"></div>
            <div id="gl_root"></div>
            <!--#macro($!main)-->
            <!--#end-->
        </div>
    </div>
<hr/>
<footer class="footer">
<div class="container">$!{gl_site.copyright}</div>
</footer>
</div>
</body>
</html>